{
  "extends": ["standard", "standard-react"],
  "parser": "babel-eslint",
  "env": {
    "mocha": true
  },
  "globals": {
    "shallow": true,
    "expect": true,
    "td": true,
    "chance": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "comma-dangle": ["error", "always-multiline"],
    "curly": ["error", "multi"]
  }
}
